    var jobs = '{"2":"Driver","3":"House Worker","4":"Male Cooker","6":"Nanny","7":"Female Nurse","8":"House Maid","9":"Male Nurse","10":"Farm Worker","11":"Cook \/ Waiter","12":"Female Cooker","14":"Male Physiotherapist","18":"Female Physiotherapist"}';
    var job_id = '';
