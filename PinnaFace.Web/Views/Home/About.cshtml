    jobs = jQuery.parseJSON(jobs);
    var male_jobs = {'2': jobs[2], '3': jobs[3], '4': jobs[4], '9': jobs[9], '11': jobs[11], '14': jobs[14]};
    var female_jobs = {'6': jobs[6], '8': jobs[8], '7': jobs[7], '12': jobs[12], '18': jobs[18]};

    function buildJobs(j) {
        $("#job_id").empty().append('<option value="">Please select</option>');
        $.each(j, function (index, value) {
            if (job_id == index) {
                $("#job_id").append('<option value="' + index + '" selected>' + value + '</option>');
            } else {
                $("#job_id").append('<option value="' + index + '">' + value + '</option>');
            }
        });
    }
    $(document).ready(function () {
        buildJobs(male_jobs);
        $('input[type=radio][name=gender]').change(function () {
            if ('1' == this.value) {// male
                var assigned_jobs = male_jobs;
            } else { // female
                var assigned_jobs = female_jobs;
            }

            buildJobs(assigned_jobs);
        });
        if (gender) {
            $('input[type=radio][name=gender]').trigger("change");
        }
    });
