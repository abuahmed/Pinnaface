@model PinnaFace.Core.Models.AgencyDTO

@{
    ViewBag.Title = @Model.AgencyName + "";
}

<div style="padding: 10px; background: white;">
    
    <h4>@ViewBag.Title</h4>
   
    <h5 style="margin-top: 25px;">Agency Detail</h5>
    <hr style="margin-bottom: 5px; margin-top: 5px;"/>
    <div class="container-fluid">
        <div class="row-fluid">
            <div class="span12">
                <div class="form-inline">
                    @Html.HiddenFor(model => model.Id)
                    
                    <div class="form-group">
                        @Html.LabelFor(model => model.LicenceNumber)
                        @Html.TextBoxFor(model => model.LicenceNumber, new {@class = "form-control", @disabled = "disabled"})
                           
                    </div>

                    <div class="form-group">
                        @Html.LabelFor(model => model.ManagerName)
                        @Html.TextBoxFor(model => model.ManagerName, new {@class = "form-control", @disabled = "disabled"})
                          
                    </div>
                </div>
               
            </div>
       
        </div>
    </div>
    
    <h5 style="margin-top: 25px;">Agency Address</h5>
    <hr style="margin-bottom: 5px; margin-top: 5px;"/>
    <div class="container-fluid">
        <div class="row-fluid">
            <div class="span12">
                <div class="form-inline">
                    <div class="form-group">
                        @Html.LabelFor(model => model.Address.Mobile)
                        @Html.TextBoxFor(model => model.Address.Mobile, new {@class = "form-control", @disabled = "disabled"})
                         
                    </div>
                    <div class="form-group">
                        @Html.LabelFor(model => model.Address.AlternateMobile)
                        @Html.TextBoxFor(model => model.Address.AlternateMobile, new {@class = "form-control", @disabled = "disabled"})
                         
                    </div>
                </div>
                <div class="form-inline">
                    <div class="form-group">
                        @Html.LabelFor(model => model.Address.Telephone)
                        @Html.TextBoxFor(model => model.Address.Telephone, new {@class = "form-control", @disabled = "disabled"})
                         
                    </div>
                    
                    <div class="form-group">
                        @Html.LabelFor(model => model.Address.AlternateTelephone)
                        @Html.TextBoxFor(model => model.Address.AlternateTelephone, new {@class = "form-control", @disabled = "disabled"})
                         
                    </div>
                </div>
                <div class="form-inline">
                    <div class="form-group">
                        @Html.LabelFor(model => model.Address.PrimaryEmail)
                        @Html.TextBoxFor(model => model.Address.PrimaryEmail, new {@class = "form-control", @disabled = "disabled"})
                         
                    </div>
                    <div class="form-group">
                        @Html.LabelFor(model => model.Address.AlternateEmail)
                        @Html.TextBoxFor(model => model.Address.AlternateEmail, new {@class = "form-control", @disabled = "disabled"})
                         
                    </div>



                </div>
            </div>
       
        </div>
    </div>

</div>